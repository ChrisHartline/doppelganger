{
  "buildCommand": "npm install --workspaces && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm install --workspaces",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
